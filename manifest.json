{
  "name": "Quicken",
  "description": "Surf faster than your mouse can handle!",
  "version": "2.2",
  "short_name": "Accelerate Keyboard Browsing",
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_title": "Open Next Page",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "commands": {
    "open-next-page": {
      "description": "Open Next Page",
      "suggested_key": {
        "default": "Ctrl+Right"
      }
    },
    "open-prev-page": {
      "description": "Open Previous Page",
      "suggested_key": {
        "default": "Ctrl+Left"
        }
    }
  },
  "content_scripts" : [
    {
      "matches" : [
        "*://*/*"
      ],
      "js" : ["element_searcher.js"],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "manifest_version": 2
}