{
  "name": "Show Next Results Page on Google Search™",
  "description": "When on a Google Search™ search page, show next results page",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "forward.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Next results page",
    "default_icon": "arrow-right-c.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Right"
      }
    }
  },
  "manifest_version": 2
}